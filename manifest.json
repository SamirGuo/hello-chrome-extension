{
    "name": "My Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Gets information from Google.",
    "icons": {
      "16": "imgs/get_started16.png",
      "32": "imgs/get_started32.png",
      "48": "imgs/get_started48.png",
      "128": "imgs/get_started128.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["background_script.js"]
    },
    "permissions": ["*", "background", "activeTab", "declarativeContent", "storage"],
    "default_locale": "en",
    "browser_action": {
        "default_icon": {
            "16": "imgs/get_started16.png",
            "32": "imgs/get_started32.png",
            "48": "imgs/get_started48.png",
            "128": "imgs/get_started128.png"
        },
        "default_popup": "popup.html",
        "default_title": "__MSG_"
    }
  }
